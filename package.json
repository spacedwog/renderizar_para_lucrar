    "@types/react-native-vector-icons": "^6.4.18",
    "expo": "^54.0.20",
    "react": "^18.2.0",
    "react-native": "^0.72.6",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.29.0",
    "react-native-image-picker": "^7.2.3",
    "react-native-permissions": "^3.10.1",
    "react-native-reanimated": "^3.5.4",
